<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <system.diagnostics>
    <sharedListeners>
      <add name="console" type="System.Diagnostics.ConsoleTraceListener" >
      </add>
    </sharedListeners>
    <sources>
      <source name="HDAClientTraceSource" switchValue="All">
        <!--
          Add listener to source. Use named switches for multiple sources; switch values can be names, decimal, or hexadecimal.
        -->
        <listeners>
          <clear />
          <add name="console" />
        </listeners>
      </source>
      <source name="ConsoleApplicationTraceSource" switchValue="All">
        <listeners>
          <clear />
          <add name="console" />
        </listeners>
      </source>
      <source name="MergerTraceSource" switchValue="All">
        <listeners>
          <clear />
          <add name="console" />
        </listeners>
      </source>
      <source name="OutputWriterTraceSource" switchValue="All">
        <listeners>
          <clear />
          <add name="console" />
        </listeners>
      </source>
    </sources>
  </system.diagnostics>
</configuration>